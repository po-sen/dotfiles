[alias]
	st = status
	co = checkout
	br = branch
	cp = cherry-pick
	fp = fetch -pPt
	amend = commit --amend --reset-author --no-edit
	tree = log --decorate --oneline --graph
	bb = !git branch --merged HEAD | grep -v \"^\\*\"
[branch]
	sort = -committerdate
[core]
	editor = nvim
[init]
	defaultBranch = master
