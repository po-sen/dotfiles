[alias]
	st = status
	co = checkout
	br = branch
	cp = cherry-pick
	fp = fetch -pt --all
	amend = commit --amend --reset-author --no-edit
	tree = log --decorate --oneline --graph
	bb = !git branch --merged HEAD | grep -v \"^\\*\"
	codex = !git branch --format='%(refname:short)' --list 'codex/*' '*-codex/*'
[branch]
	sort = -committerdate
[core]
	editor = nvim
[init]
	defaultBranch = master
[user]
	email = posen.chiu@pentium.network
	name = Posen
